{
  "name": "tutoring-fitness-platform",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8000",
  "scripts": {
    "start": "node build",
    "all": "yarn && cd expressServer && yarn",
    "client": "node scripts/start.js",
    "expressServer": "cd expressServer && nodemon server.js",
    "start-dev": "concurrently \"yarn client\" \"yarn expressServer\"",
    "build": "run-s build:*",
    "build:client": "node scripts/build.js",
    "build:server": "NODE_ENV=production yarn webpack --config ./server/webpack.config.js",
    "predeploy": "run-s --print-label predeploy:*",
    "predeploy:build": "run-s build",
    "predeploy:create-artifact": "node scripts/createEbArtifact.js",
    "postinstall": "run-s postinstall:*",
    "postinstall:graphql": "run-s download-schema relay",
    "test": "run-s --print-label test:*",
    "test:eslint": "eslint \"src/**/*.@(js|jsx)\"",
    "test:prettier": "prettier \"src/**/*.@(css|json)\" --list-different",
    "test:jest": "node ./scripts/test.js",
    "test:flow": "flow check",
    "relay": "relay-compiler",
    "relay-watch": "relay-compiler --watch",
    "download-schema": "node ./scripts/downloadSchema.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.32",
    "@material-ui/styles": "^4.6.0",
    "@reach/rect": "^0.2.1",
    "@reduxjs/toolkit": "^1.0.4",
    "@ungap/url-search-params": "^0.1.2",
    "abortcontroller-polyfill": "^1.3.0",
    "axios": "^0.19.0",
    "clipboard-copy": "^3.1.0",
    "clsx": "^1.0.4",
    "concurrently": "^5.0.2",
    "date-fns": "^1.30.1",
    "express": "^4.17.1",
    "extract-files": "^5.0.1",
    "flat": "^5.0.0",
    "graphql-request": "^1.8.2",
    "i": "^0.3.6",
    "i18n-iso-countries": "^4.3.1",
    "invariant": "^2.2.4",
    "jsdom": "^15.2.0",
    "keyboardjs": "^2.5.1",
    "lodash": "^4.17.15",
    "luxon": "^1.17.2",
    "npm": "^6.13.4",
    "react": "^16.9.0",
    "react-app-polyfill": "^1.0.1",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.9.0",
    "react-helmet-async": "^1.0.3",
    "react-images-upload": "^1.2.7",
    "react-infinite-scroller": "^1.2.4",
    "react-intl": "^3.6.2",
    "react-native-web": "^0.11.7",
    "react-phone-input-2": "^2.10.1",
    "react-redux": "7.1.1",
    "react-relay": "^7.1.0",
    "react-relay-network-modern": "^4.2.0",
    "react-router-dom": "^5.1.0",
    "react-rte": "^0.16.1",
    "react-select": "^3.0.4",
    "react-share": "^3.0.1",
    "react-use": "^12.2.0",
    "react-vis": "^1.11.7",
    "react-web-gifted-chat": "^0.6.7",
    "rebound": "^0.1.0",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "relay-hooks": "^2.0.0-rc.7",
    "relay-runtime": "^7.1.0",
    "remark-parse": "^7.0.1",
    "remark-react": "^6.0.0",
    "serve": "^11.1.0",
    "twilio-chat": "^3.3.2",
    "unified": "^8.3.2",
    "uuid": "^3.3.3",
    "whatwg-url": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@mdx-js/loader": "^1.1.0",
    "@svgr/webpack": "4.3.2",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-relay": "^7.1.0",
    "babel-preset-react-app": "^9.0.0",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "3.1.0",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-flowtype": "^3.10.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "file-loader": "4.1.0",
    "flow-bin": "^0.112.0",
    "flow-typed": "^2.6.2",
    "fs-extra": "7.0.1",
    "glob": "^7.1.4",
    "glob-promise": "^3.4.0",
    "graphql": "^14.2.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.8.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.8.0",
    "jest-watch-typeahead": "0.3.1",
    "json": "^9.0.6",
    "jszip": "^3.2.2",
    "mini-css-extract-plugin": "0.8.0",
    "node-fetch": "^3.1.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.4.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.18.2",
    "react-dev-utils": "^9.0.1",
    "react-testing-library": "^6.1.2",
    "relay-compiler": "^7.1.0",
    "relay-config": "^7.1.0",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "semver": "6.0.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.3.0",
    "ts-pnp": "1.1.2",
    "url-loader": "2.1.0",
    "webpack": "4.36.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "3.7.2",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "4.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "@babel/proposal-optional-chaining",
      "@babel/proposal-nullish-coalescing-operator",
      "relay"
    ]
  }
}
